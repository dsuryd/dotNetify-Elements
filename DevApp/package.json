      "dotnetify": "^3.0.1",
      "marked": "~0.4.0",
      "moment": "~2.22.2",
      "prismjs": "~1.15.0",
      "prop-types": "~15.6.1",
      "react": "~16.3.2",
      "react-chartjs-2": "~2.7.2",
      "react-data-grid": "~4.0.7",
      "react-dom": "~16.3.2",
      "react-widgets": "~4.3.2",
      "react-widgets-moment": "~4.0.6",
      "reactstrap": "~6.1.0",
