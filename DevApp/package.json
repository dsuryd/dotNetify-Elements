{
  "version": "1.0.0-dev",
  "name": "dotnetify-elements-dev-app",
  "private": true,
  "scripts": {
    "build": "webpack",
    "prod": "webpack -p --progress && dotnet publish -c Release",
    "dist": "webpack --config ./webpack.dist.config.js",
    "test": "webpack --config ./webpack.test.config.js",
    "pretty": "prettier --write src/**/*.js"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "babel-plugin-styled-components",
      "babel-plugin-transform-object-rest-spread",
      "babel-plugin-transform-class-properties"
    ]
  },
  "prettier": {
    "trailingComma": "none",
    "arrowParens": "avoid",
    "tabWidth": 2,
    "singleQuote": false
  },
  "dependencies": {
    "chart.js": "~2.8.0",
    "chartjs-plugin-streaming": "~1.8.0",
    "chartjs-plugin-zoom": "~0.7.5",
    "dotnetify": "~3.4.0",
    "emotion": "~10.0.9",
    "html-to-react": "~1.3.4",
    "marked": "~0.7.0",
    "moment": "~2.24.0",
    "prismjs": "~1.15.0",
    "prop-types": "~15.6.1",
    "quill": "~1.3.6",
    "react": "~16.8.0",
    "react-chartjs-2": "~2.8.0",
    "react-data-grid": "~4.0.7",
    "react-dom": "~16.8.0",
    "react-widgets": "~4.3.2",
    "react-widgets-moment": "~4.0.6",
    "reactstrap": "~6.1.0",
    "styled-components": "~4.1.3",
    "text-mask-addons": "~3.7.2",
    "text-mask-core": "~5.1.1"
  },
  "devDependencies": {
    "@material-ui/core": "1.2.2",
    "aspnet-webpack": "3.0.0",
    "awesome-typescript-loader": "5.1.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "0.28.11",
    "deepmerge": "2.1.1",
    "file-loader": "1.1.11",
    "mini-css-extract-plugin": "~0.4.0",
    "prettier": "2.0.5",
    "raw-loader": "^2.0.0",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.12.0",
    "webpack-bundle-analyzer": "3.4.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.2"
  }
}
